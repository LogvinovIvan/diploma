<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.76-ea"
          xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="by.bsuir.fksis.poit.obfuscator.controller.SyntaxPageController">
    <columnConstraints>
        <ColumnConstraints/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints/>
    </rowConstraints>
    <children>
        <Pane prefHeight="331.0" prefWidth="468.0">
            <children>
                <Button id="buttonChoseFile" layoutX="44.0" layoutY="234.0" mnemonicParsing="false"
                        onAction="#handlebuttonChooseFile" text="Выбрать  файл"/>
                <CheckBox id="removeCommentCheckbox" fx:id="removeCommentCheckbox" layoutX="294.0" layoutY="53.0"
                          mnemonicParsing="false" onAction="#handleRemoveComment" text="Удалить комметарии"/>
                <CheckBox fx:id="removeJavaDocCheckbox" layoutX="294.0" layoutY="83.0" mnemonicParsing="false"
                          onAction="#handleJavaDocCheckbox" text="Удалить JavaDoc"/>
                <CheckBox fx:id="removeSpaceCheckbox" layoutX="294.0" layoutY="110.0" mnemonicParsing="false"
                          onAction="#handleSpaceObfuscator" text="Удалить форматирование"/>
                <Button fx:id="obfuscateButton" layoutX="294.0" layoutY="234.0" mnemonicParsing="false"
                        onAction="#handleObfuscateButton" text="Обфусцировать"/>
            </children>
        </Pane>
    </children>
</GridPane>
