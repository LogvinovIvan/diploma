                <CheckBox fx:id="renameMethodCheckBox" layoutX="79.0" layoutY="106.0" mnemonicParsing="false"
                          onAction="#handleRenameMethodCheckBox" prefHeight="25.0" prefWidth="166.0"
                          text="Rename methods"/>
                <CheckBox fx:id="renameClassCheckBox" layoutX="79.0" layoutY="145.0" mnemonicParsing="false"
                          onAction="#handleRanameClassCheckBox" prefHeight="17.0" prefWidth="166.0"
                          text="Rename classes"/>
            </children>
        </Pane>
    </children>
</AnchorPane>
